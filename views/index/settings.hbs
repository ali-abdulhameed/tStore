      <div class="container contact-form">

                  <div id="settings-form">
                      <h3>Settings</h3>
                     <div class="row">
                          <div class="col-md-6">
                              <div class="form-group">
                                  <input type="text"  id="firstName" class="form-control" placeholder="First Name *" value="{{user.userFirstName}}" />
                              </div>
                              <div class="form-group">
                                  <input type="text"  id="lastName" class="form-control" placeholder="Last Name *" value="{{user.userLastName}}" />
                              </div>
                              <div class="form-group">
                                  <input type="text"  id="phoneNumber" class="form-control" placeholder="Phone Number" value="{{user.userPhoneNumber}}" />
                              </div>
                              <div class="form-group">
                                  <input type="text" id="email" class="form-control" placeholder="Email" value="{{user.userEmail}}" disabled/>
                              </div>
                              <div class="form-group">
                                  <button type="button" onclick="update()" id="update" class="btnContact">Update</button>
                              </div>
                          </div>
                          <div class="col-md-6">
                            <div class="form-group">
                                <input type="text" id="street" class="form-control" placeholder="Street *" value="{{user.userStreet}}" />
                            </div>
                            <div class="form-group">
                                <input type="text" id="city" class="form-control" placeholder="City *" value="{{user.userCity}}" />
                            </div>
                            <div class="form-group">
                                <input type="text" id="state" class="form-control" placeholder="State *" value="{{user.userState}}" />
                            </div>
                            <div class="form-group">
                                <input type="text" id="zipCode" class="form-control" placeholder="zipCode *" value="{{user.userZipcode}}" />
                            </div>
                          </div>
                      </div>

                  </div>
                  <p id="errorMessage" style="color: red; text-align: center; visibility: hidden"></p>

      </div>



